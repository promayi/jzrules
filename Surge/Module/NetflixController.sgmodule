#!name=NetflixController
#!desc=Netflix策略控制器



[Script]
#应该修改的字段 netflixGroup
NetflixController = type=generic, script-path=https://raw.githubusercontent.com/promayi/jzrules/master/Surge/Module/netflixcontroller.js, timeout=60, argument=icon1=checkmark.circle&color1=55ba94&icon2=checkmark.circle.trianglebadge.exclamationmark&color2=#9a9ced&icon3=hand.raised.circle&color3=#ea5532&netflixGroup=Netflix
NetflixChecker = type=cron,cronexp=*/15 * * * *,wake-system=0,timeout=60,script-path=https://raw.githubusercontent.com/promayi/jzrules/master/Surge/Module/netflixchecker.js,script-update-interval=86400,control-api=1
